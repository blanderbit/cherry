{
  "name": "universal-demo",
  "version": "7.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng run universal-demo:serve:proxy --watch",
    "ssr": "npm run build:universal && npm run generate:prerender && npm run server",
    "ssr:debug": "ng build --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true  && ng run universal-demo:server:dev && webpack && node server.js",
    "ssr:cw": "ng build --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --watch",
    "ssr:sw": "ng run universal-demo:server:dev --watch",
    "ssr:webpack": "webpack --watch",
    "ssr:server": "nodemon server.js",
    "ssr:watch": "run-p ssr:universal:build:* ",
    "ssr:universal:build:browser": "ng run universal-demo:build:dev --watch",
    "ssr:universal:build:server": "node ./node_modules/npm-delay 15000 && ng run universal-demo:server:dev --watch",
    "ssr:universal:build:webpack": "node ./node_modules/npm-delay 30000 && webpack --config webpack.config.js --watch",
    "ssr:universal:build:nodemon": "node ./node_modules/npm-delay 35000 && nodemon --inspect server.js",
    "format:check": "prettier --write --config ./.prettierrc --list-different \"src/{app,environments,styles}/**/*{.ts,.json,.scss}\"",
    "build": "ng build",
    "generate-icons": "icon-font-generator src/assets/svg/*.svg -o src/assets/svg",
    "build:server": "ng run universal-demo:server:production",
    "build:prod": "ng build --prod",
    "pack": "npm run generate-icons && npm run build:universal && npm run generate:prerender && cp -a dist dist-server gantt static ./server.js .env app",
    "build:universal": "ng run universal-demo:build:production && ng run universal-demo:server:production && webpack --progress --colors",
    "server": "node server.js",
    "test": "ng test",
    "test:prod": " node --max_old_space_size=8192 \"node_modules/@angular/cli/bin/ng\" test --progress false --watch=false",
    "test:ssr": "run-p test:ssr:*",
    "test:ssr:server": "cross-env TEST=true node server.js",
    "test:ssr:script": "node ./node_modules/npm-delay 2000 && ts-mocha test/**/*.spec.ts test/exit.ts --timeout 15000",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prerender": "npm run build:universal && npm run generate:prerender",
    "generate:prerender": "node prerender.js",
    "bundle-report": "ng build --stats-json && webpack-bundle-analyzer dist/stats.json",
    "bundle-report:prod": "ng build --stats-json --prod --vendor-chunk && webpack-bundle-analyzer dist/stats.json",
    "sentry:build": "ng build sentry && npm i dist/sentry --save-optional",
    "library:build": "npm run sentry:build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/material": "~8.2.3",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/pwa": "0.800.0",
    "@angular/router": "8.2.14",
    "@angular/service-worker": "8.2.14",
    "@gorniv/ngx-universal": "2.0.1",
    "@ng-bootstrap/ng-bootstrap": "5.3.0",
    "@nguniversal/common": "8.1.1",
    "@nguniversal/express-engine": "8.2.6",
    "@ngx-meta/core": "~8.0.2",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@sentry/browser": "5.6.3",
    "ag-grid-angular": "22.0.0",
    "ag-grid-community": "22.0.0",
    "bootstrap": "4.3.1",
    "cookie-parser": "1.4.4",
    "core-js": "3.1.3",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "icon-font-generator": "2.1.10",
    "moment": "2.24.0",
    "ngx-autosize": "~1.8.0",
    "ngx-bootstrap-slider": "1.7.0",
    "ngx-infinite-scroll": "8.0.2",
    "ngx-take-until-destroy": "5.4.0",
    "ngx-toastr": "~11.3.3",
    "reflect-metadata": "0.1.13",
    "request": "2.88.2",
    "rxjs": "6.5.5",
    "rxjs-tslint": "^0.1.8",
    "underscore": "^1.9.2",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.28",
    "@angular-devkit/build-ng-packagr": "~0.803.28",
    "@angular/cli": "~8.3.28",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@angular/platform-server": "~8.2.14",
    "@types/jasmine": "3.3.13",
    "@types/jasminewd2": "2.0.6",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^14.0.14",
    "@types/underscore": "^1.9.4",
    "@types/webpack-env": "^1.15.2",
    "chai": "4.2.0",
    "codelyzer": "5.0.1",
    "cross-env": "5.2.0",
    "husky": "^4.2.3",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "mocha": "6.1.4",
    "ng-packagr": "5.7.1",
    "nodemon": "1.19.1",
    "npm-delay": "1.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "1.17.1",
    "prettier-tslint": "0.4.2",
    "protractor": "5.4.2",
    "source-map-support": "0.5.12",
    "ssri": "6.0.1",
    "ts-mocha": "6.0.0",
    "ts-node": "8.2.0",
    "tsickle": "0.37.0",
    "tslib": "^1.11.1",
    "tslint": "5.16.0",
    "typescript": "~3.5.3",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "1.7.2"
  },
  "optionalDependencies": {
    "menu": "file:dist/menu",
    "sentry": "file:dist/sentry"
  }
}
